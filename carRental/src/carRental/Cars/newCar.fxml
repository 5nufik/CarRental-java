<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onMouseClicked="#clear" prefHeight="470.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carRental.Cars.newCarColtroller">
    <children>
        <Label fx:id="label" alignment="CENTER" layoutX="15.0" layoutY="15.0" prefHeight="30.0" prefWidth="350.0" text="Новая машина">
            <font>
                <Font name="Microsoft Sans Serif" size="24.0" />
            </font>
        </Label>

        <Label alignment="CENTER_RIGHT" layoutX="15.0" layoutY="60.0" prefHeight="25.0" prefWidth="140.0" text="Модель">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <ComboBox fx:id="comboMark" layoutX="165.0" layoutY="60.0" prefHeight="25.0" prefWidth="215.0" promptText="Выберите..." visibleRowCount="5" />

        <Label alignment="CENTER_RIGHT" layoutX="15.0" layoutY="100.0" prefHeight="25.0" prefWidth="140.0" text="Тип ТС">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <ComboBox fx:id="comboType" layoutX="165.0" layoutY="100.0" prefHeight="25.0" prefWidth="215.0" promptText="Выберите..." visibleRowCount="5" />

        <Label alignment="CENTER_RIGHT" layoutX="15.0" layoutY="137.0" prefHeight="25.0" prefWidth="140.0" text="Коробка передач">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <ComboBox fx:id="comboTransmission" layoutX="165.0" layoutY="137.0" prefHeight="25.0" prefWidth="215.0" promptText="Выберите..." visibleRowCount="5" />

        <Label alignment="CENTER_RIGHT" layoutX="10.0" layoutY="180.0" prefHeight="25.0" prefWidth="140.0" text="Цвет">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <TextField fx:id="txtColor" layoutX="165.0" layoutY="180.0" prefHeight="20.0" prefWidth="215.0">
            <font>
                <Font name="Microsoft Sans Serif" size="12.0" />
            </font>
        </TextField>

        <Label alignment="CENTER_RIGHT" layoutX="10.0" layoutY="220.0" prefHeight="25.0" prefWidth="140.0" text="Год выпуска">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <TextField fx:id="txtYear" layoutX="165.0" layoutY="220.0" prefHeight="20.0" prefWidth="215.0">
            <font>
                <Font name="Microsoft Sans Serif" size="12.0" />
            </font>
        </TextField>

        <Label alignment="CENTER_RIGHT" layoutX="10.0" layoutY="260.0" prefHeight="25.0" prefWidth="140.0" text="Мощность, л.с.">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <TextField fx:id="txtPower" layoutX="165.0" layoutY="260.0" prefHeight="20.0" prefWidth="215.0">
            <font>
                <Font name="Microsoft Sans Serif" size="12.0" />
            </font>
        </TextField>

        <Label alignment="CENTER_RIGHT" layoutX="10.0" layoutY="300.0" prefHeight="25.0" prefWidth="140.0" text="Госномер">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <TextField fx:id="txtPlate" layoutX="165.0" layoutY="300.0" prefHeight="20.0" prefWidth="215.0">
            <font>
                <Font name="Microsoft Sans Serif" size="12.0" />
            </font>
        </TextField>

        <Label alignment="CENTER_RIGHT" layoutX="10.0" layoutY="340.0" prefHeight="25.0" prefWidth="140.0" text="VIN номер">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <TextField fx:id="txtVIN" layoutX="165.0" layoutY="340.0" prefHeight="20.0" prefWidth="215.0">
            <font>
                <Font name="Microsoft Sans Serif" size="12.0" />
            </font>
        </TextField>

        <Label alignment="CENTER_RIGHT" layoutX="10.0" layoutY="380.0" prefHeight="25.0" prefWidth="140.0" text="Страховой полис">
            <font>
                <Font name="Microsoft Sans Serif" size="17.0" />
            </font>
        </Label>
        <TextField fx:id="txtInsurance" layoutX="165.0" layoutY="380.0" prefHeight="20.0" prefWidth="215.0">
            <font>
                <Font name="Microsoft Sans Serif" size="12.0" />
            </font>
        </TextField>

        <Button layoutX="105.0" layoutY="420.0" mnemonicParsing="false" onAction="#add" prefHeight="35.0" prefWidth="90.0" text="Добавить" textAlignment="CENTER">
            <font>
                <Font name="Microsoft Sans Serif" size="14.0" />
            </font>
        </Button>

        <Button layoutX="205.0" layoutY="420.0" mnemonicParsing="false" onAction="#close" prefHeight="35.0" prefWidth="90.0" text="Отменить" textAlignment="CENTER">
            <font>
                <Font name="Microsoft Sans Serif" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
